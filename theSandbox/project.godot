; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="theSandbox"
run/main_scene="res://world_scenes/system/system.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

BlockData="*res://autoload/BlockData.gd"
ItemData="*res://autoload/ItemData.gd"
CraftData="*res://autoload/CraftData.gd"
GlobalRef="*res://autoload/GlobalRef.gd"
PlayerData="*res://autoload/PlayerData.gd"

[debug]

settings/stdout/print_fps=true
gdscript/warnings/unused_parameter=0
gdscript/warnings/integer_division=0

[display]

window/size/viewport_width=400
window/size/viewport_height=300
window/size/window_width_override=800
window/size/window_height_override=600
window/stretch/mode="viewport"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[file_customization]

folder_colors={
"res://addons/": "gray",
"res://autoload/": "red",
"res://bin/": "orange",
"res://block_resources/": "yellow",
"res://item_resources/": "green",
"res://object_scenes/": "blue",
"res://ui_scenes/": "purple",
"res://world_scenes/": "pink",
"res://zeeg/": "gray"
}

[gui]

fonts/dynamic_fonts/use_oversampling=false
theme/default_font_antialiasing=0

