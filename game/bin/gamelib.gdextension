[configuration]

entry_symbol = "lib_init"
compatibility_minimum = "4.4"
reloadable = true

[libraries]

macos.debug = "res://bin/libgamecore.macos.template_debug.framework"
macos.release = "res://bin/libgamecore.macos.template_release.framework"

windows.debug.x86_32 = "res://bin/libgamecore.windows.template_debug.x86_32.dll"
